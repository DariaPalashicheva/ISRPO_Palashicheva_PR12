<Page x:Class="ISRPO_Palashicheva_PR12.PageStudent.PageAccountStudent"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ISRPO_Palashicheva_PR12.PageStudent"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Аккаунт студента">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Left">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="Имя студента" Style="{StaticResource Title}"/>
                <TextBlock Text="Имя" Style="{StaticResource Title}"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="Логин студента" Style="{StaticResource Title}"/>
                <TextBlock Text="Логин" Style="{StaticResource Title}"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
