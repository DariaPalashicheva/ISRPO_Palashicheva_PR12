<Window x:Class="ISRPO_Palashicheva_PR12.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ISRPO_Palashicheva_PR12"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="350"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <Image Source="Resources/icon.png" Width="50"/>
            <TextBlock Text="Канцелярские товары" Style="{StaticResource Title}"/>
        </StackPanel>
        <Frame x:Name="FrmMain" NavigationUIVisibility="Hidden" Grid.Row="1">
            
        </Frame>
    </Grid>
</Window>
